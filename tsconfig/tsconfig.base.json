{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noEmitHelpers": true,
        "esModuleInterop": true,
        "importHelpers": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
    },
}
