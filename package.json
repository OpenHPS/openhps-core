{
  "name": "@openhps/core",
  "version": "0.1.0",
  "description": "Open Hybrid Positioning System",
  "author": "Maxim Van de Wynckel",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.wise.vub.ac.be/mvdewync/openhps-core.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha",
    "test:jenkins": "jenkins-mocha",
    "cover": "nyc mocha && nyc report --reporter=cobertura",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "clean": "node ./scripts/clean.js",
    "build": "npm run clean && npm run build:typescript && npm run build:typedoc",
    "build:typescript": "tsc",
    "build:typedoc": "typedoc --excludePrivate --hideGenerator --out docs src",
    "release": "semantic-release --branch origin/production"
  },
  "keywords": [
    "openhps",
    "ips",
    "slam",
    "hybrid",
    "positioning",
    "system"
  ],
  "dependencies": {
    "mathjs": "^6.2.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mathjs": "^6.0.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.11",
    "@types/uuid": "^3.4.6",
    "chai": "^4.2.0",
    "jenkins-mocha": "^8.0.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.31",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.1",
    "tslint-no-circular-imports": "^0.7.0",
    "typedoc": "^0.15.1",
    "typescript": "^3.7.2"
  },
  "nyc": {
    "branches": 20,
    "lines": 20,
    "functions": 20,
    "statements": 20,
    "include": [
      "src"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  }
}
