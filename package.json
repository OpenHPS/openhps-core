{
  "name": "@openhps/core",
  "version": "0.1.0-alpha.85",
  "description": "Open Hybrid Positioning System - Core component",
  "author": "Maxim Van de Wynckel",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenHPS/openhps-core.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "unpkg": "dist/openhps-core.min.js",
  "jsdelivr": "dist/openhps-core.min.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "mocha --experimental-worker",
    "test:jenkins": "jenkins-mocha --cobertura",
    "benchmark": "ts-node test/bench/worker.bench.ts",
    "cover": "nyc mocha && nyc report --reporter=cobertura",
    "lint": "eslint src/**",
    "clean": "shx rm -rf ./dist && shx rm -rf ./docs",
    "build": "npm run clean && npm run build:three && npm run build:typescript && npm run build:webpack",
    "build:typedoc": "typedoc --exclude **/_internal/**/* --excludePrivate --hideGenerator --out docs src",
    "build:typescript": "tsc",
    "build:webpack": "webpack --env.dev --progress -- && webpack --env.prod --progress --",
    "build:three": "shx mkdir -p ./dist/three/ && shx cp -r node_modules/three/src/* ./dist/three/ && babel ./dist/three/* --out-dir . --verbose --presets @babel/preset-env  --relative",
    "publish:development": "standard-version --prerelease alpha -m 'chore(alpha): %s' --author='ci <ci@openhps.org>' && npm publish --tag dev",
    "publish:beta": "standard-version --prerelease beta -m 'chore(beta): %s' --author='ci <ci@openhps.org>' && npm publish --tag beta",
    "publish:release": "standard-version -m 'chore(release): %s' --author='ci <ci@openhps.org>' && npm publish --tag latest"
  },
  "keywords": [
    "openhps",
    "hybrid",
    "positioning",
    "system"
  ],
  "dependencies": {
    "reflect-metadata": ">=0.1.13",
    "threads": ">=1.6.3",
    "typedjson": ">=1.5.2",
    "uuid": "^8.3.1",
    "worker-thread": ">=1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/benchmark": "^1.0.33",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.19.6",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "csv-parser": "^2.3.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prettier": ">=3.1.4",
    "husky": "^4.3.0",
    "jenkins-mocha": "^8.0.0",
    "microtime": "^3.0.0",
    "mocha": "^6.2.3",
    "nyc": "^14.1.1",
    "prettier": "^2.2.0",
    "shx": "^0.3.3",
    "standard-version": "^8.0.2",
    "terser-webpack-plugin": "^3.1.0",
    "threads-plugin": "^1.3.3",
    "three": "^0.120.1",
    "ts-node": "^8.10.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.11",
    "webpack-inject-plugin": "^1.5.5"
  },
  "nyc": {
    "branches": 50,
    "lines": 50,
    "functions": 50,
    "statements": 50,
    "include": [
      "src"
    ],
    "exclude": [
      "test",
      "src/**/_internal/Worker*.ts",
      "src/**/WorkerService.ts",
      "src/service/_internal/DummyDataService.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
