{
  "name": "@openhps/core",
  "version": "0.1.0-alpha.71",
  "description": "Open Hybrid Positioning System - Core component",
  "author": "Maxim Van de Wynckel",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenHPS/openhps-core.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/openhps-core.min.js",
  "jsdelivr": "dist/openhps-core.min.js",
  "scripts": {
    "test": "mocha --experimental-worker",
    "test:jenkins": "jenkins-mocha --cobertura",
    "cover": "nyc mocha && nyc report --reporter=cobertura",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "clean": "shx rm -rf ./dist && shx rm -rf ./docs",
    "build": "npm run clean && npm-run-all --parallel build:typescript build:typedoc && npm run build:webpack",
    "build:typescript": "tsc --module es2015",
    "build:typedoc": "typedoc --exclude **/_internal/**/* --excludePrivate --hideGenerator --out docs src",
    "build:webpack": "webpack",
    "publish:development": "standard-version --prerelease alpha -m 'chore(alpha): %s' --author='ci <ci@openhps.org>' && npm publish --tag dev --access public",
    "publish:beta": "standard-version --prerelease beta -m 'chore(beta): %s' --author='ci <ci@openhps.org>' && npm publish --tag beta --access public",
    "publish:release": "standard-version -m 'chore(release): %s' --author='ci <ci@openhps.org>' && npm publish --tag latest --access public"
  },
  "keywords": [
    "openhps",
    "ips",
    "hybrid",
    "positioning",
    "system"
  ],
  "dependencies": {
    "reflect-metadata": ">=0.1.13",
    "threads": ">=1.6.3",
    "three": ">=0.119.1",
    "typedjson": ">=1.5.1",
    "uuid": ">=8.3.0",
    "worker-thread": ">=1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.11.2",
    "@commitlint/cli": ">=9.1.1",
    "@commitlint/config-conventional": ">=9.1.1",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.54",
    "@types/three": "^0.103.2",
    "@types/uuid": "^8.0.1",
    "chai": "^4.2.0",
    "csv-parser": "^2.3.2",
    "husky": "^4.0.10",
    "jenkins-mocha": "^8.0.0",
    "mocha": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "shx": "^0.3.2",
    "standard-version": "^8.0.2",
    "terser-webpack-plugin": "^3.1.0",
    "threads-plugin": "^1.3.2",
    "ts-node": "^8.10.1",
    "tslint": "^5.20.1",
    "tslint-no-circular-imports": "^0.7.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-auto-inject-version": "^1.2.2",
    "webpack-cli": "^3.3.11"
  },
  "nyc": {
    "branches": 50,
    "lines": 50,
    "functions": 50,
    "statements": 50,
    "include": [
      "src"
    ],
    "exclude": [
      "test",
      "src/**/_internal/Worker*.ts",
      "src/**/WorkerService.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --no-verify"
    }
  }
}
